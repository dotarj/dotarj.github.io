@model UsingViewModelsWithAspNetMvc.ViewModels.CompanyViewModel

<table>
    <tr>
        <th>Name</th>
        <th>Birth date</th>
        <th>Salary</th>
    </tr>
    @foreach (var employee in Model.Employees)
    {
        <tr>
            <td>@employee.Name</td>
            <td>@employee.BirthDate</td>
            <td>@employee.Salary</td>
        </tr>
    }
</table>